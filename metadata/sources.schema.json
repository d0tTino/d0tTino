{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Source Entry",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {"type": "string"},
      "url": {"type": "string", "format": "uri"},
      "category": {"type": "string"},
      "tags": {
        "type": "array",
        "items": {"type": "string"}
      },
      "license": {"type": "string"}
    },
    "required": ["name", "url", "category", "tags", "license"],
    "additionalProperties": false
  }
}
